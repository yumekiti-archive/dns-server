; web ip
@   IN  A   192.168.200.12

; dns ip
ns  IN  A   192.168.200.11

www IN  CNAME   stocklist-rice.com.
web IN  CNAME   stocklist-rice.com.
dns IN  CNAME   ns.stocklist-rice.com.